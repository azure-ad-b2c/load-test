          <JSR223Sampler guiclass="TestBeanGUI" testclass="JSR223Sampler" testname="Generate Code Verifier and Code Challenge" enabled="true">
            <stringProp name="cacheKey">true</stringProp>
            <stringProp name="filename"></stringProp>
            <stringProp name="parameters"></stringProp>
            <stringProp name="script">import org.apache.commons.lang.RandomStringUtils
import org.apache.commons.codec.binary.Base64

String charset = ((&apos;A&apos;..&apos;Z&apos;) + (&apos;0&apos;..&apos;9&apos;)).join()
Integer length = 50
String verifier = RandomStringUtils.random(length, charset.toCharArray())

vars.put(&quot;code_verifier&quot;, verifier)
log.warn &apos;Random Code Verifier: &apos; + vars.get(&quot;code_verifier&quot;)     


byte[] bytes = verifier.getBytes(&quot;UTF-8&quot;)
byte[] challengeBytes = org.apache.commons.codec.digest.DigestUtils.sha256(bytes)
String challengeBase64 = Base64.encodeBase64URLSafeString(challengeBytes)

vars.put(&quot;code_challenge&quot;, challengeBase64)
log.warn &apos;Code Challenge: &apos; + vars.get(&quot;code_challenge&quot;)     
</stringProp>
            <stringProp name="scriptLanguage">groovy</stringProp>
          </JSR223Sampler>
